{
  "version": "0.2.0",
  "configurations": [
    {
        "type": "node",
        "request": "launch",
        "name": "Debug Serverless Offline",
        "cwd": "${workspaceFolder}/apis",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "args": [
            "run",
            "debug"
        ],
        "sourceMaps": true,
        "runtimeExecutable": "npm",
        "console": "integratedTerminal"
    },
  ]
}